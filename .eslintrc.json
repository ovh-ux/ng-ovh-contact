{
    "extends": "./node_modules/eslint-config-ovh/configs/es5-browser.js",
    "rules": {
        "no-mixed-operators": "warn",
        "max-len": "warn",
        "consistent-return": "off",
        "no-param-reassign": "warn",
        "newline-per-chained-call": "warn",
        "no-shadow": "warn"
    },
    "globals": {
        "require": true,
        "module": true
    },
    "env": {
        "jasmine": true
    }
}
